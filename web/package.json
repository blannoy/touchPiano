{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.2",
    "copy-files-from-to": "^3.11.0",
    "env-cmd": "^10.1.0",
    "node-sass": "^7.0.3",
    "react": "^18.2.0",
    "react-data-table-component": "^7.6.2",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^6.1.6",
    "react-native-input-spinner": "^1.8.1",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "react-scroll-direction": "^1.0.0",
    "react-slider": "^2.0.6",
    "react-use-scroll-direction": "^0.1.0",
    "recharts": "^2.12.4",
    "sass": "^1.76.0",
    "sass-loader": "^14.2.1",
    "smplr": "^0.14.0",
    "standardized-audio-context": "^25.3.72",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "env-cmd -f .env.acc react-scripts start",
    "image": "env-cmd -f .env.image react-scripts build",
    "build": "env-cmd -f .env.local react-scripts build",
    "postbuild": "copy-files-from-to --mode=local",
    "postimage": "copy-files-from-to --mode=image"
  },
  "copyFiles": [
    {
        "from": "../config/config.json",
        "to": {
                "default": {
                    "skip": true
                },
          "image": "../data/"
      }
    },
    {
      "from": {
          "default": {
              "skip": true
          },
          "start": "../sounds/**",
          "local": "../sounds/**"
      },
      "to": {
        "default": {
          "skip": true
      },
        "start": "build/sounds/",
        "local": "dist/sounds/"
      }
  }
],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
